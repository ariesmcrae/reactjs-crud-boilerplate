{
  "name": "reactjs-crud-boilerplate",
  "description": "ReactJS CRUD Boilerplate",
  "version": "0.0.2",
  "main": "index.js",
  "repository": "https://github.com/ariesmcrae/reactjs-crud-boilerplate.git",
  "author": "Aries McRae <github@ariesmcrae.com>",
  "license": "MIT",
  "keywords": [
    "Boilerplate",
    "Starter Kit",
    "Starter Project",
    "Seed"
  ],
  "scripts": {
    "prestart": "babel-node scripts/startMessage.js",
    "start": "npm-run-all --parallel security-check lint:watch test:watch startServerDev",
    "startServerDev": "babel-node ./scripts/serverDev.js",
    "security-check": "nsp check",
    "lint": "esw webpack.config.* src scripts --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "jest",
    "test:watch": "yarn test --env=jsdom -- --watch",
    "coverage": "jest --coverage --env=jsdom",
    "clean-dist": "node_modules/.bin/rimraf dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node scripts/build.js",
    "postbuild": "babel-node scripts/serverProd.js"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-bootstrap-table": "^3.1.5",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.6.1",
    "redux-thunk": "^2.2.0",
    "tether": "^1.4.0",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "coveralls": "^2.13.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.0.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jsdom": "^9.12.0",
    "nock": "^9.0.11",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2",
    "open": "^0.0.5",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.16.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "^0.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost"
  }
}
